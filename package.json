{
  "name": "store-ratings-fullstack",
  "version": "1.0.0",
  "description": "Full-stack store ratings application",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend/client && npm install",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend/client && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd frontend/client && npm run build",
    "start": "cd backend && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["express", "react", "postgresql", "fullstack"],
  "author": "",
  "license": "MIT"
} 